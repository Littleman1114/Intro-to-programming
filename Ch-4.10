tweet = input('Enter abbreviation from tweet:\n')

if tweet == 'LOL':
    print('LOL = laughing out loud')
elif tweet == 'BFN':
    print('BFN = bye for now')
elif tweet == 'FTW':
    print('FTW = for the win')
elif tweet == 'IRL':
    print('IRL = in real life')
elif tweet == 'WTF':
    print('WTF = Whens the foxtrot?')
elif tweet == 'THX':
    print('THX = Thank you')
elif tweet == 'TTYL':
    print('TTYL = Talk to you later')
elif tweet == 'BRB':
    print('BRB = Be Right Back')
else:
    print("Sorry, don't know that one")
tweet = input('Enter tweet 160 characters or less')
if len(tweet) > 160:
    print('Error! Only 160 or less characters allowed!')
else:
    print('\nGood Job')
